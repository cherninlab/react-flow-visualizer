{
  "name": "vite-react-flow-template",
  "version": "1.0.0",
  "keywords": [
    "react",
    "graph",
    "diagram",
    "workflow",
    "react-flow"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
